{
  "name": "capn-hook",
  "packageManager": "yarn@3.2.3",
  "type": "module",
  "main": "./build/index.js",
  "scripts": {
    "lint": "prettier --check . && eslint src --ext mjs,js,ts",
    "format": "prettier --write . && eslint src --ext mjs,js,ts --fix",
    "fmt": "yarn run format",
    "clean": "rimraf build",
    "compile": "tsc",
    "check": "yarn run compile --noEmit",
    "build": "yarn run clean && yarn run compile",
    "vitest": "vitest run",
    "test": "yarn run check && yarn run lint && yarn run vitest",
    "dev": "yarn run clean && yarn run compile --watch",
    "debug": "nodemon --watch build --inspect",
    "postinstall": "husky install",
    "schemagen": "yarn run build && node . schemagen"
  },
  "devDependencies": {
    "@types/http-status-codes": "^1.2.0",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.25",
    "eslint": "^8.24.0",
    "eslint-config-neon": "^0.1.33",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "@lolpants/jogger": "^0.6.0",
    "discord.js": "^13.6.0",
    "dot-prop": "^7.2.0",
    "http-status-codes": "^2.2.0",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "mkdirp": "^1.0.4",
    "source-map-support": "^0.5.21",
    "striptags": "^3.2.0",
    "zod": "^3.14.4",
    "zod-to-json-schema": "^3.11.3"
  }
}
