{
  "name": "capn-hook",
  "packageManager": "yarn@3.2.3",
  "type": "module",
  "main": "./build/index.js",
  "scripts": {
    "test": "yarn run check && yarn run lint && yarn run ava",
    "check": "yarn run compile --noEmit",
    "lint": "xo",
    "compile": "tsc",
    "clean": "rimraf build",
    "prebuild": "yarn run clean",
    "build": "yarn run compile",
    "dev": "yarn run build --watch",
    "debug": "nodemon --inspect",
    "postinstall": "husky install",
    "schemagen": "yarn run build && node . schemagen"
  },
  "devDependencies": {
    "@types/http-status-codes": "^1.2.0",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.25",
    "ava": "^4.2.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3",
    "xo": "^0.48.0"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "@lolpants/jogger": "^0.6.0",
    "discord.js": "^13.6.0",
    "dot-prop": "^7.2.0",
    "http-status-codes": "^2.2.0",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "mkdirp": "^1.0.4",
    "source-map-support": "^0.5.21",
    "striptags": "^3.2.0",
    "zod": "^3.14.4",
    "zod-to-json-schema": "^3.11.3"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
